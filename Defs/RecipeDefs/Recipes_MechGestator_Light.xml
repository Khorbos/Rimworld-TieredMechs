<Defs>

  <!-- Lifter+ -->
  <RecipeDef ParentName="LightMechanoidRecipe">
    <defName>LifterPlus</defName>
    <label>gestate advanced lifter</label>
    <description>Gestate an advanced lifter mechanoid.</description>
    <ingredients Inherit="False">
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>125</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>200</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>12</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SubcoreHigh</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <Mech_LifterPlus>1</Mech_LifterPlus>
    </products>
    <descriptionHyperlinks>
      <ThingDef>Mech_LifterPlus</ThingDef>
    </descriptionHyperlinks>
	<researchPrerequisite>HighMechtech</researchPrerequisite>
  </RecipeDef>

